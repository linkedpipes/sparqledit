@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.co-ode.org/ontologies/pizza/pizza.owl>
  a owl:Ontology ;
  rdfs:comment "An example ontology that contains all constructs required for the various versions of the Pizza Tutorial run by Manchester University (see http://www.co-ode.org/resources/tutorials/)"@en ;
  owl:versionInfo "v.1.5. Removed protege.owl import and references. Made ontology URI date-independent"@en, "version 1.5"^^xsd:string, "v.1.4. Added Food class (used in domain/range of hasIngredient), Added several hasCountryOfOrigin restrictions on pizzas, Made hasTopping invers functional"@en .

<http://www.co-ode.org/ontologies/pizza/pizza.owl#hasBase>
  a owl:ObjectProperty, owl:InverseFunctionalProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasIngredient> ;
  rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#PizzaBase> ;
  rdfs:domain <http://www.co-ode.org/ontologies/pizza/pizza.owl#Pizza> ;
  owl:inverseOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#isBaseOf> .

<http://www.co-ode.org/ontologies/pizza/pizza.owl#hasCountryOfOrigin> a owl:ObjectProperty .
<http://www.co-ode.org/ontologies/pizza/pizza.owl#hasIngredient>
  a owl:ObjectProperty, owl:TransitiveProperty ;
  rdfs:comment "NB Transitive - the ingredients of ingredients are ingredients of the whole"@en ;
  owl:inverseOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf> ;
  rdfs:domain <http://www.co-ode.org/ontologies/pizza/pizza.owl#Food> ;
  rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#Food> .

<http://www.co-ode.org/ontologies/pizza/pizza.owl#hasSpiciness>
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#Spiciness> ;
  rdfs:comment "A property created to be used with the ValuePartition - Spiciness."@en .

<http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping>
  a owl:ObjectProperty, owl:InverseFunctionalProperty ;
  rdfs:comment "Note that hasTopping is inverse functional because isToppingOf is functional"@en ;
  rdfs:domain <http://www.co-ode.org/ontologies/pizza/pizza.owl#Pizza> ;
  rdfs:subPropertyOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasIngredient> ;
  rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#PizzaTopping> ;
  owl:inverseOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#isToppingOf> .

<http://www.co-ode.org/ontologies/pizza/pizza.owl#isBaseOf>
  a owl:ObjectProperty, owl:InverseFunctionalProperty, owl:FunctionalProperty ;
  rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#Pizza> ;
  rdfs:domain <http://www.co-ode.org/ontologies/pizza/pizza.owl#PizzaBase> ;
  rdfs:subPropertyOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf> .

<http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf>
  a owl:ObjectProperty, owl:TransitiveProperty ;
  rdfs:domain <http://www.co-ode.org/ontologies/pizza/pizza.owl#Food> ;
  rdfs:comment "The inverse property tree to hasIngredient - all subproperties and attributes of the properties should reflect those under hasIngredient."@en ;
  rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#Food> .

<http://www.co-ode.org/ontologies/pizza/pizza.owl#isToppingOf>
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf> ;
  rdfs:comment "Any given instance of topping should only be added to a single pizza (no cheap half-measures on our pizzas)"@en ;
  rdfs:domain <http://www.co-ode.org/ontologies/pizza/pizza.owl#PizzaTopping> ;
  rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#Pizza> .

<http://www.co-ode.org/ontologies/pizza/pizza.owl#American>
  a owl:Class ;
  rdfs:label "Americana"@pt ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping> ;
    owl:someValuesFrom <http://www.co-ode.org/ontologies/pizza/pizza.owl#TomatoTopping>
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping> ;
    owl:allValuesFrom [
      a owl:Class ;
      owl:unionOf (
       <http://www.co-ode.org/ontologies/pizza/pizza.owl#MozzarellaTopping>
       <http://www.co-ode.org/ontologies/pizza/pizza.owl#TomatoTopping>
       <http://www.co-ode.org/ontologies/pizza/pizza.owl#PeperoniSausageTopping>
     )
    ]
  ], <http://www.co-ode.org/ontologies/pizza/pizza.owl#NamedPizza>, [
    a owl:Restriction ;
    owl:onProperty <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping> ;
    owl:someValuesFrom <http://www.co-ode.org/ontologies/pizza/pizza.owl#PeperoniSausageTopping>
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasCountryOfOrigin> ;
    owl:hasValue <http://www.co-ode.org/ontologies/pizza/pizza.owl#America>
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping> ;
    owl:someValuesFrom <http://www.co-ode.org/ontologies/pizza/pizza.owl#MozzarellaTopping>
  ] ;
  owl:disjointWith <http://www.co-ode.org/ontologies/pizza/pizza.owl#QuattroFormaggi>, <http://www.co-ode.org/ontologies/pizza/pizza.owl#AmericanHot> .

<http://www.co-ode.org/ontologies/pizza/pizza.owl#AmericanHot>
  a owl:Class ;
  rdfs:label "AmericanaPicante"@pt ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping> ;
    owl:allValuesFrom [
      a owl:Class ;
      owl:unionOf (
